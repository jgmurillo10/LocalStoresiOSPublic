# Uncomment the next line to define a global platform for your project
platform :ios, '9.0'

target 'LocalStoresiOS' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for LocalStoresiOS
  pod 'Firebase/Core'
  pod 'Firebase/Auth'
  pod 'Firebase/Firestore'
  pod 'GoogleMaps', '2.2'
  pod 'SwiftKeychainWrapper'
  pod 'NewRelicAgent'

  target 'LocalStoresiOSTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'LocalStoresiOSUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
